{
  "name": "git_commit_demo",
  "version": "1.0.0",
  "description": "commitizen,commitlint,semantic-release",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "test": "echo \"Error: no test specified\" && exit 1",
    "changelog": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangfen/git_commit_demo.git"
  },
  "keywords": [
    "git",
    "commit",
    "commintizen",
    "commitlint",
    "semantic-release"
  ],
  "author": "miyatang <tangfen19149@163.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tangfen/git_commit_demo/issues"
  },
  "homepage": "https://github.com/tangfen/git_commit_demo#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "standard-version": "^7.0.0"
  }
}
